{
  "name": "discourse-changelog",
  "version": "0.0.0",
  "private": true,
  "description": "Small description for discourse-changelog goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "vite build && BUILD_SSR=true vite build && node ./scripts/prerender.mjs",
    "build:data": "concurrently 'node lib/write-commit-info.mjs' 'node lib/fetch-new-features.mjs' 'node lib/fetch-security-advisories.mjs'",
    "build:data:advisories": "node lib/fetch-security-advisories.mjs",
    "build:full": "pnpm build:data && pnpm build",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"pnpm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"pnpm:lint:css --fix\"",
    "lint:fix": "concurrently \"pnpm:lint:*:fix\" --names \"fix:\" --prefixColors auto && pnpm format",
    "lint:format": "prettier . --cache --check",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "vite",
    "test": "vite build --mode development && ember test --path dist"
  },
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./app/*"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@babel/runtime": "^7.28.4",
    "@ember/optional-features": "^2.3.0",
    "@ember/string": "^4.0.1",
    "@ember/test-helpers": "^5.4.1",
    "@ember/test-waiters": "^4.1.1",
    "@embroider/compat": "^4.1.12",
    "@embroider/config-meta-loader": "^1.0.0",
    "@embroider/core": "^4.4.2",
    "@embroider/legacy-inspector-support": "^0.1.3",
    "@embroider/macros": "^1.19.6",
    "@embroider/router": "^3.0.6",
    "@embroider/vite": "^1.5.0",
    "@eslint/js": "^9.39.2",
    "@glimmer/component": "^2.0.0",
    "@rollup/plugin-babel": "^6.1.0",
    "babel-plugin-ember-template-compilation": "^2.4.1",
    "concurrently": "^9.2.1",
    "decorator-transforms": "^2.3.1",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.8.1",
    "ember-cli-babel": "^8.2.0",
    "ember-load-initializers": "^3.0.1",
    "ember-modifier": "^4.2.2",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resolver": "^13.1.1",
    "ember-source": "~6.8.2",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-ember": "^12.7.5",
    "eslint-plugin-n": "^17.23.1",
    "eslint-plugin-qunit": "^8.2.5",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "prettier-plugin-ember-template-tag": "^2.1.2",
    "qunit": "^2.24.3",
    "qunit-dom": "^3.5.0",
    "stylelint": "^16.26.1",
    "stylelint-config-standard": "^38.0.0",
    "testem": "^3.17.0",
    "tracked-built-ins": "^4.1.0",
    "vite": "8.0.0-beta.5"
  },
  "engines": {
    "node": ">= 20"
  },
  "ember": {
    "edition": "octane"
  },
  "dependencies": {
    "@discourse/lint-configs": "^2.36.0",
    "@eslint/compat": "^2.0.0",
    "@glint/ember-tsc": "^1.0.8",
    "@html-next/vertical-collection": "^4.0.2",
    "@isomorphic-git/lightning-fs": "^4.6.2",
    "execa": "^9.6.1",
    "isomorphic-git": "^1.36.1",
    "jsdom": "^27.4.0",
    "semver": "^7.7.3",
    "simple-dom": "^1.4.0"
  }
}
